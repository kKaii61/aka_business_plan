<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="stock_warehouse_location_transfer_view_form" model="ir.ui.view">
            <field name="name">stock.warehouse.location.transfer.form.inherit</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='wh_output_stock_loc_id']" position="after">
                    <field name="x_location_transfer_id" options="{'no_quick_create': True, 'no_create_edit': True}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
