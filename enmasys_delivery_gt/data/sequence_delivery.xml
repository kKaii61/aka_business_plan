<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Sequences for sale.order -->
        <record id="seq_tradition_delivery" model="ir.sequence">
            <field name="name">traditiondelivery import</field>
            <field name="code">tradition.delivery.sequence</field>
            <field name="prefix">PGHGT/%(year)s%(month)s%(day)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>